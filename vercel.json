{
  "version": 2,
  "outputDirectory": "Mycredimedia-main/dist",
  "builds": [
    {
      "src": "Mycredimedia-main/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}